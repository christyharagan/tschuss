{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "declaration": true,
        "module": "commonjs",
        "noImplicitAny": false,
        "noLib": false,
        "rootDir": ".",
        "sourceMap": true,
        "sourceRoot": ".",
        "target": "es5",
        "outDir": "./dist"
    },
    "filesGlob": [
        "./lib/**/*.ts",
        "./examples/**/*.ts",
        "./node_modules/*/*.d.ts",
        "./node_modules/typescript/bin/typescript.d.ts",
        "./tschuss.d.ts"
    ],
    "compileOnSave": true,
    "files": [
        "./lib/decorators.ts",
        "./lib/index.ts",
        "./lib/injector.ts",
        "./lib/model.ts",
        "./lib/modelGenerator.ts",
        "./lib/wiringResolution.ts",
        "./examples/basic.ts",
        "./node_modules/typescript-schema/typescript-schema.d.ts",
        "./node_modules/typescript/bin/typescript.d.ts",
        "./tschuss.d.ts"
    ]
}
